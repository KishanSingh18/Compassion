                            android:layout_width="109dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:paddingLeft="8dp"
                            android:paddingBottom="3dp"
                            android:text="3 years of sponsorship Thank You"
